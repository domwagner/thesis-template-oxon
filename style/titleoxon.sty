% be typeset at the columbidae University
% Please check for updates
\ProvidesPackage{titleoxon}[2015/08/10 v.01 an
example package^^J for wikibooks]
\RequirePackage{graphicx}
\RequirePackage{setspace}
\newcommand*{\uni}[1]{\gdef\@uni{#1}}
\newcommand*{\@uni}{University of Oxford}
\newcommand*{\college}[1]{\gdef\@college{#1}}
\newcommand*{\@college}{Magdalen College}
\newcommand*{\logo}[1]{\gdef\@logo{#1}}
\newcommand*{\@logo}{ox.png}
\newcommand*{\department}[1]{\gdef\@department{#1}}
\newcommand*{\@department}{Department of Computer Science}
\newcommand*{\degree}[1]{\gdef\@degree{#1}}
\newcommand*{\@degree}{\texttt{\string\degree} currently not set. Please fix this.}
\newcommand*{\term}[1]{\gdef\@term{#1}}
\newcommand*{\@term}{\texttt{\string\term} currently not set. Please fix this.}

\newcommand*{\supervisor}[1]{\gdef\@supervisor{#1}}
\newcommand*{\@supervisor}{\texttt{\string\supervisor} currently
not set. Please fix this.}

\renewcommand*{\maketitle}{%


\begin{titlepage}
  \begin{center}
    \vspace*{0.5cm}
    \textsc{\LARGE\@uni}\\[0.2cm]
    \@department\\[1.3cm]


%    Title
    \vspace{-0.5cm}

    \begin{spacing}{1.9}
      {\LARGE\bfseries\@title} \\[1.4cm]
    \end{spacing}
    
    \vspace{-0.5cm}
    \includegraphics[width=4cm]{\@logo}
    \vspace{-0.5cm}
    \\[1.8cm]


    % Author and supervisor
    \begin{normalsize}
      \emph{submitted by}\\[0.2cm]
      \vspace{-0.1cm}
      {\Large\@author}\\[0.15cm]
      {\@college}\\[0.35cm]
      \emph{on} \@date
      \\[1.2cm]
      
      \vspace{2mm}
      \emph{Supervisor} \\[0.2cm]
      \@supervisor\\[22mm]
      
    \emph{A thesis submitted in partial fulfilment of the requirements for the degree of}\\
    \@degree\\[0.4cm]
    \@term
    \end{normalsize}
    \vspace{1cm}

  \end{center}

\end{titlepage}

}
\endinput